<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:dam="http://www.day.com/dam/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="cq:Template"
    allowedPaths="[/content/entities(/.*)?]"
    ranking="{Long}100">
    <jcr:content
        cq:isDelivered="{Boolean}false"
        cq:scaffolding="/conf/diriyah/settings/dam/cfm/models/diriyah-image-content-fragment-model/jcr:content/model"
        cq:templateType="/libs/settings/dam/cfm/model-types/fragment"
        jcr:primaryType="cq:PageContent"
        jcr:title="Diriyah image content fragment model"
        sling:resourceSuperType="dam/cfm/models/console/components/data/entity"
        sling:resourceType="dam/cfm/models/console/components/data/entity/default"
        isLocked="{Boolean}false"
        status="enabled">
        <metadata
            dam:scene7PublishPending="{Boolean}true"
            jcr:primaryType="nt:unstructured"/>
        <model
            cq:targetPath="/content/entities"
            jcr:primaryType="cq:PageContent"
            sling:resourceType="wcm/scaffolding/components/scaffolding"
            dataTypesConfig="/mnt/overlay/settings/dam/cfm/models/formbuilderconfig/datatypes"
            maxGeneratedOrder="20">
            <cq:dialog
                jcr:primaryType="nt:unstructured"
                sling:resourceType="cq/gui/components/authoring/dialog">
                <content
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="granite/ui/components/coral/foundation/fixedcolumns">
                    <items
                        jcr:primaryType="nt:unstructured"
                        maxGeneratedOrder="21">
                        <_x0031_747653074428
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                            fieldLabel="Image Alt Text"
                            listOrder="1"
                            maxlength="255"
                            metaType="text-single"
                            name="imageAltText"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            valueType="string">
                            <granite:data jcr:primaryType="nt:unstructured"/>
                        </_x0031_747653074428>
                        <_x0031_753877875085
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="dam/cfm/models/editor/components/contentreference"
                            fieldLabel="Image"
                            filter="hierarchy"
                            listOrder="21"
                            metaType="reference"
                            name="image"
                            nameSuffix="contentReference"
                            renderReadOnly="false"
                            showEmptyInReadOnly="true"
                            showThumbnail="false"
                            valueType="string/reference">
                            <granite:data
                                jcr:primaryType="nt:unstructured"
                                thumbnail-validation="cfm.validation.thumbnail.show"/>
                        </_x0031_753877875085>
                    </items>
                </content>
            </cq:dialog>
        </model>
        <change-register jcr:primaryType="nt:unstructured"/>
    </jcr:content>
</jcr:root>
